<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	version="1.0">
	<persistence-unit name="visionbizsolutionsJPAUnit"
		transaction-type="RESOURCE_LOCAL">
		<class>com.visionbizsolutions.orm.jpa.bean.User</class>
		<class>com.visionbizsolutions.orm.jpa.bean.UserRole</class>
		<class>com.visionbizsolutions.orm.jpa.bean.UserFile</class>
		<class>com.visionbizsolutions.orm.jpa.bean.UserAttempts</class>
		
		<exclude-unlisted-classes />

		<properties>
			<property name="hibernate.ejb.naming_strategy" value="org.hibernate.cfg.ImprovedNamingStrategy" />
			<property name="hibernate.cache.provider_class" value="org.hibernate.cache.HashtableCacheProvider" />
		</properties>

	</persistence-unit>
</persistence>